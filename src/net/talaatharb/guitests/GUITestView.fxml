<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<SplitPane dividerPositions="0.5" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.talaatharb.guitests.GUITestController">
   <items>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button layoutX="13.0" layoutY="14.0" mnemonicParsing="false" onAction="#addCircle" text="Add Circle" />
            <Button layoutX="10.0" layoutY="48.0" mnemonicParsing="false" onAction="#addButton" text="Add Button" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="pane" prefHeight="300.0" prefWidth="300.0">
         <children>
            <Circle fx:id="circle" fill="DODGERBLUE" layoutX="150.0" layoutY="150.0" radius="17.0" stroke="BLACK" strokeType="INSIDE" />
            <Button fx:id="testButton" layoutX="112.0" layoutY="88.0" mnemonicParsing="false" text="Test Button" />
         </children>
      </AnchorPane>
   </items>
</SplitPane>
